web: java $JAVA_OPTS -Dserver.port=$PORT -Dspring.profiles.active=$ACTIVE_PROFILE -Dsendemail.service.mailgun.mailgunKey=$MAILGUN_KEY -Dsendemail.service.mailgun.mailgunDomain=$MAILGUN_DOMAIN -Dspring.datasource.url=$POSTGRE_URI -Dspring.datasource.username=$POSTGRE_USER -Dspring.datasource.password=$POSTGRE_PASS -jar target/*.jar